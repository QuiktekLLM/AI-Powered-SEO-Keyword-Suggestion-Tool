{
  "name": "ai-powered-seo-keyword-tool",
  "version": "1.0.0",
  "description": "AI-Powered SEO Keyword Suggestion Tool with comprehensive unit tests",
  "main": "scripts.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "serve": "npx serve .",
    "start": "npx serve ."
  },
  "keywords": [
    "seo",
    "keywords",
    "ai",
    "cloudflare",
    "javascript"
  ],
  "author": "kedster",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@jest/globals": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"],
    "collectCoverageFrom": [
      "scripts.js",
      "cloudflare-worker.js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "coverageReporters": ["text", "lcov", "html"],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}